<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Taehyun.Activity.Mypage_NoCardActivity">
    <LinearLayout
        android:id="@+id/card_firstll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        >
        <ImageView
            android:id="@+id/nocard_firstbtn"
            android:src="@drawable/card_enrooool"
            android:layout_width="300dp"
            android:layout_height="120dp"
            android:layout_marginTop="250dp"

            />
        <ImageView
            android:id="@+id/nocard_cardadd_btn"
            android:src="@drawable/card_enroll"
            android:layout_width="300dp"
            android:layout_height="80dp"
            android:layout_marginTop="30dp"
            />

    </LinearLayout>
</LinearLayout>